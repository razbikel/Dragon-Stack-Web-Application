CH_SUCCESS:\r\n            return { \r\n                ...state,\r\n                status: fetchStates.success,\r\n                rangeFilter: action.rangeFilter\r\n            };\r\n\r\n        case SEARCH_CURRENT_PAGE.FETCH_SUCCESS:\r\n            return { \r\n                ...state,\r\n                status: fetchStates.success,\r\n                currentPage: action.currentPage\r\n            };\r\n            \r\n        default:\r\n            return state;\r\n        }\r\n}\r\n\r\nexport default searchReducer;"},"lineCount":null}},"hash":"2764f7b65c42445fa4401151a58e345f","cacheData":{"env":{}}}