lumn":30}},{"generated":{"line":101,"column":0},"source":"actions\\account.js","original":{"line":85,"column":28}},{"generated":{"line":101,"column":5},"source":"actions\\account.js","original":{"line":85,"column":11}},{"generated":{"line":101,"column":6},"source":"actions\\account.js","original":{"line":85,"column":4}},{"generated":{"line":102,"column":0},"source":"actions\\account.js","original":{"line":92,"column":1}},{"generated":{"line":102,"column":1},"source":"actions\\account.js","original":{"line":84,"column":7}}],"sources":{"actions\\account.js":"import {ACCOUNT} from './types';\r\nimport {BACKEND} from '../config';\r\n\r\n\r\nexport const fetchFromAccount = ({ endpoint, options, FETCH_TYPE, ERROR_TYPE, SUCCESS_TYPE }) => {\r\n    return (\r\n        (dispatch) => {\r\n            dispatch({type: FETCH_TYPE});\r\n            return (\r\n                fetch(`${BACKEND.ADDRESS}/account/${endpoint}`, options)\r\n                .then(res => res.json())\r\n                .then( json => {\r\n                    if (json.type === 'error'){\r\n                        dispatch({\r\n                            type: ERROR_TYPE,\r\n                            message: json.message\r\n                        });\r\n                    }\r\n                    else {\r\n                        dispatch({\r\n                            type: SUCCESS_TYPE,\r\n                            ...json\r\n                        })\r\n                    }\r\n                })\r\n                .catch(error => dispatch({\r\n                    type: ERROR_TYPE,\r\n                    message: error.message\r\n                    }))\r\n            )\r\n        }\r\n    )\r\n}\r\n\r\n\r\nexport const signup = ({ username, password }) => { \r\n    return (\r\n        fetchFromAccount({\r\n            endpoint: 'signup',\r\n            options: {\r\n                method: 'POST',\r\n                body: JSON.stringify({ username, password }),\r\n                headers: {'Content-Type': 'application/json'},\r\n                credentials: 'include'\r\n            },\r\n            FETCH_TYPE: ACCOUNT.FETCH,\r\n            ERROR_TYPE: ACCOUNT.FETCH_ERROR,\r\n            SUCCESS_TYPE: ACCOUNT.FETCH_SUCCESS\r\n        })\r\n    );\r\n}\r\n\r\n\r\nexport const login = ({ username, password }) => { \r\n    return (\r\n        fetchFromAccount({\r\n            endpoint: 'login',\r\n            options: {\r\n                method: 'POST',\r\n                body: JSON.stringify({ username, password }),\r\n                headers: {'Content-Type': 'application/json'},\r\n                credentials: 'include'\r\n            },\r\n            FETCH_TYPE: ACCOUNT.FETCH,\r\n            ERROR_TYPE: ACCOUNT.FETCH_ERROR,\r\n            SUCCESS_TYPE: ACCOUNT.FETCH_SUCCESS\r\n    \r\n        })\r\n    )\r\n}\r\n\r\n\r\nexport const logout = () => fetchFromAccount({\r\n    endpoint: 'logout',\r\n    options: {\r\n        credentials: 'include'\r\n    },\r\n    FETCH_TYPE: ACCOUNT.FETCH,\r\n    ERROR_TYPE: ACCOUNT.FETCH_ERROR,\r\n    SUCCESS_TYPE: ACCOUNT.FETCH_LOGOUT_SUCCESS\r\n});\r\n\r\n\r\nexport const fetchAuthenticated = () => {\r\n    return fetchFromAccount({\r\n        endpoint: 'authenticated',\r\n        options: { credentials: 'include' },\r\n        FETCH_TYPE: ACCOUNT.FETCH,\r\n        ERROR_TYPE: ACCOUNT.FETCH_ERROR,\r\n        SUCCESS_TYPE: ACCOUNT.FETCH_AUTHENTICATED_SUCCESS\r\n    })\r\n}\r\n\r\n"},"lineCount":null}},"hash":"38aaaa6984518008561cfe2e4e734755","cacheData":{"env":{}}}